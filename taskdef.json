{
    "family": "test",
    "containerDefinitions": [
        {
            "image": "122160613195.dkr.ecr.us-east-2.amazonaws.com/test:latest",
            "name": "test",
            "cpu": 10,
            "memory": 256,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ],
            "mountPoints": [
                {
                    "containerPath": "/test",
                    "sourceVolume": "test"
                }
          ]
        }
    ],
  "volumes": [
    {
      "name": "test",
      "host": {
        "sourcePath": "/test"
      }
    }
  ]
}
