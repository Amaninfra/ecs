{
   "family": "test",	
   "volumes": [
    {
      "name": "applicationData",
      "host": {
        "sourcePath": "/opt/aman"
      }
    }
  ],
"containerDefinitions": [
        {
            "image": "122160613195.dkr.ecr.us-east-2.amazonaws.com/test:latest",
            "name": "test",
            "cpu": 10,
            "memory": 256,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ],
	"mountPoints": [
        {
          "sourceVolume": "applicationData",
          "containerPath": "/root/www"
        }
      ]
        }
    ]
}
