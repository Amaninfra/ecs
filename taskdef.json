{
    "family": "test",
    "containerDefinitions": [
        {
            "image": "122160613195.dkr.ecr.us-east-2.amazonaws.com/test:latest",
            "name": "test",
            "cpu": 10,
            "memory": 256,
            "essential": true,
			"volumes": [
        {
            "host": {
                "sourcePath": "/mnt"
            },
            "name": "mnt"
        }
    ],
	
  "mountPoints": [
        {
          "readOnly": false,
          "containerPath": "/root/www",
          "sourceVolume": "mnt"
        }
      ],
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ]
        }
    ]
}
