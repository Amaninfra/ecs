{
  "family": "test",
  "volumes": [
    {
      "name": "my",
      "host": {
        "sourcePath": "/opt"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "test",
      "image": "122160613195.dkr.ecr.us-east-2.amazonaws.com/test:latest",
      "cpu": 100,
      "memory": 500,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "my",
          "containerPath": "/root/www"
        }
      ],
      "essential": true
    },
